    "docker-typeorm:revert": "docker-compose exec app node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert -d src/shared/infra/typeorm/index.ts",
    "typeorm:run": "NODE_ENV=test node --require ts-node/register ./node_modules/typeorm/cli.js migration:run -d src/shared/infra/typeorm/index.ts",
    "typeorm:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert -d src/shared/infra/typeorm/index.ts",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles"
  },
